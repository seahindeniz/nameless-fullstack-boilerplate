{
  "name": "@nameless-fullstack-boilerplate/web",
  "version": "0.0.1",
  "description": "Front-end app for the nameless boiler project",
  "author": "Sahin Deniz <seahindeniz@gmail.com> (https://github.com/seahindeniz)",
  "private": true,
  "main": "src/index.tsx",
  "engines": {
    "node": ">=10.17.0"
  },
  "license": "ISC",
  "repository": "github:seahindeniz/nameless-fullstack-boilerplate",
  "scripts": {
    "build": "cross-env TS_NODE_PROJECT=\"tsconfig.webpack.json\" webpack --config webpack.prod.ts --progress",
    "serve": "serve -n -l 3001",
    "start": "pnpm run build && pnpm run serve",
    "dev": "webpack-cli serve --config webpack.dev.ts --hot --port 3001",
    "-": "",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook",
    "--": "",
    "test": "jest --detectOpenHandles --forceExit",
    "test:watch": "pnpm test -- --watch",
    "test:watchAll": "pnpm test -- --watchAll",
    "---": "",
    "lint": "eslint \"./src/**/*.{ts,tsx}\"",
    "lint:ci": "pnpm run lint -- --quiet",
    "lint:fix": "pnpm run lint -- --fix",
    "----": "",
    "dock": "docker-compose up -d --build",
    "make-lock": "make-dedicated-lockfile"
  },
  "dependencies": {
    "@emotion/react": "^11.4.0",
    "@emotion/styled": "^11.3.0",
